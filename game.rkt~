#lang racket
(require 2htdp/image)
(require 2htdp/universe)
(require lang/posn)

(struct game (walls player enemies) #:transparent)
; walls : hash
; player : player
; enemies : Listof Enemy 

(struct entity (x y hp) #:transparent)
; where:
; x : Integer
; y : Integer
; hp : Integer

; player extends entity
(struct player entity (str items) #:transparent)
; where
; str: Integer
; items: Listof Item


; enemy extends entity
(struct enemy entity (str items))
(player 1 1 1 "10" "sword")


